RewriteEngine On

# Redirect .html â†’ clean URL (canonical)
RewriteCond %{THE_REQUEST} \s+/foildata/([^\s]+)\.html
RewriteRule ^ /foildata/%1 [R=301,L]

# Internally serve .html files
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !\.(php|json|js|css|png|jpg|svg|webp)$
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.+)$ $1.html [L]